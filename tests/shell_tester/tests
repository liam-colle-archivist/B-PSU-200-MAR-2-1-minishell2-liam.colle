[8.3.1.2]
NAME="ERROR HANDLING (Wrong builtin): (setenv [Wrong keys])"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'setenv 42SH'
  echo 'setenv SH42-'
  echo 'setenv SH450$*ùù:2+7/'
  echo 'setenv SH42'
  echo 'setenv SH42_SH'
  echo 'setenv I am 42SH'
  echo 'setenv 42SH'
  echo 'setenv SH42-2'
  echo 'setenv SH450$*ùù:2+7/2'
  echo 'setenv SH42'
  echo 'setenv SH42_SH'
  echo 'setenv "I am 42SH2"'
  echo 'env'

[8.3.1.2-END]

