  unsetenv OLDPWD
  echo 'cd /usr'
  echo 'pwd'
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd -'
  echo 'pwd'
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
[5.3.2-END]

