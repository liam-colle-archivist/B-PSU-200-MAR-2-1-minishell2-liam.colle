[10.1.1]
NAME="REDIRECTION: Simple redirection '>'"
SETUP="export TERM=xterm; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'echo "hello world" > out_test'
  echo 'cat out_test'
[10.1.1-END]

[10.1.2]
NAME="REDIRECTION: Simple redirection '>>'"
SETUP="export TERM=xterm; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'echo "hello world" > out_test'
  echo 'echo "pipes are annoying" >> out_test'
  echo 'cat out_test'
[10.1.2-END]

[10.2.1]
NAME="REDIRECTION: Simple redirection '<'"
SETUP="export TERM=xterm; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'echo "hello world" > out_test'
  echo 'echo "pipes are annoying" >> out_test'
  echo 'cat < out_test'
[10.2.1-END]

